  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.3",
    "fast-xml-parser": "^4.1.3",
    "form-data": "^4.0.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "log4js": "^6.9.1",
    "mkdirp": "^2.1.6",
    "nedb-promises": "^6.2.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.55.0",
