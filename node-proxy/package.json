    "scripts": [
      "ncc/index.js",
      "ncc/PRGAThreadCom.js"
    ],
    "assets": [
      "ncc/public/**/*",
      "ncc/prebuilds/**/*",
      "ncc/build/**/*",
      "./node_modules/axios/dist/node/axios.cjs"
